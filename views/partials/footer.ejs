<script type="text/javascript">
 $( "input[name='qty']").each(function(){
 	//alert($this.attr("max"));

$(this).on("keyup change paste", function() {
	if(($(this).val()) < ($(this).attr("min"))){
$(this).val('');
	}
	if($(this).attr("max")!=""){
	var inval = $(this).val();
	 var max = $(this).attr("max");
       if(max-inval<0){
      	$(this).next(".btn-sm").attr("disabled",true);
      	if ($('.err-qty').length) {
   //do something if elem is present
} else {
    $("<span class='err-qty'>Limit exceeded than "+ $(this).attr('max') +"</span>").insertAfter(this);
}

       }
       else{
       		$(this).next(".btn-sm").attr("disabled",false);
       		        $('.err-qty').remove();

       }
         }
          });
 });

 function checkPasswordMatch() {
    var password = $("#password").val();
    var confirmPassword = $("#password-confirm").val();

    if (password != confirmPassword)
        $("#divCheckPasswordMatch").html("<span style='text-align: center; background: #efefef; MARGIN: 0 auto; padding: 5px 46px; color:red' >Password does not match!</span>");
    else
        $("#divCheckPasswordMatch").html("<span style='text-align: center; background: #efefef; MARGIN: 0 auto; padding: 5px 46px; color:green' >Passwords match.</span>");
}

$(document).ready(function () {
   $("#password-confirm").keyup(checkPasswordMatch);
   
});

   
</script>

